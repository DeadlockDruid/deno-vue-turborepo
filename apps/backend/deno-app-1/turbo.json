{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/bundle.js"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.{spec,test}.{js,ts,jsx,tsx,snap}",
        "!package.json",
        "!turbo.json",
        "!test_setup.{js,ts}"
      ],
      "cache": true
    },
    "lint": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.{spec,test}.{js,ts,jsx,tsx,snap}",
        "!package.json",
        "!turbo.json",
        "!test_setup.{js,ts}"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!package.json",
        "!turbo.json"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": true
    },
    "format": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.{spec,test}.{js,ts,jsx,tsx,snap}",
        "!package.json",
        "!turbo.json",
        "!deno.json",
        "!deno.lock"
      ],
      "cache": true
    },
    "deploy": {
      "cache": false,
      "outputs": []
    }
  }
}
