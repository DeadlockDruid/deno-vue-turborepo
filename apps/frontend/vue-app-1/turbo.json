{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!vite.config.ts",
        "!vitest.config.ts",
        "!node_modules/**",
        "!.turbo/**",
        "!.vscode/**",
        "!eslint.config.js",
        "!tsconfig.node.json",
        "!tsconfig.spec.json",
        "!**/*.spec.ts",
        "!**/*.test.ts",
        "!**/*.md",
        "!**/*.log",
        "!**/docs/**",
        "!**/notes/**",
        "!.editorconfig",
        "!.prettierrc",
        "!.prettierignore",
        "!.cache/**",
        "!dist/**",
        "!build/**",
        "!.scannerwork/**",
        "!codeql-database/**",
        "!pnpm-lock.yaml",
        "!package-lock.json",
        "!swa-cli.config.json"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!node_modules/**",
        "!.turbo/**",
        "!.vscode/**",
        "!tsconfig.node.json",
        "!tsconfig.spec.json",
        "!**/*.spec.ts",
        "!**/*.test.ts",
        "!**/*.md",
        "!**/*.log",
        "!**/docs/**",
        "!**/notes/**",
        "!.editorconfig",
        "!.prettierrc",
        "!.prettierignore",
        "!.cache/**",
        "!dist/**",
        "!build/**",
        "!.scannerwork/**",
        "!codeql-database/**",
        "!pnpm-lock.yaml",
        "!package-lock.json",
        "!vite.config.ts",
        "!vitest.config.ts",
        "!**/*.d.ts",
        "!swa-cli.config.json"
      ],
      "cache": true
    },
    "format": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "!node_modules/**",
        "!.turbo/**",
        "!.vscode/**",
        "!**/*.log",
        "!tsconfig.node.json",
        "!tsconfig.spec.json",
        "!tsconfig.app.json",
        "!.editorconfig",
        "!.prettierrc",
        "!.prettierignore",
        "!.cache/**",
        "!dist/**",
        "!build/**",
        "!.scannerwork/**",
        "!codeql-database/**",
        "!pnpm-lock.yaml",
        "!package-lock.json",
        "!vite.config.ts",
        "!vitest.config.ts",
        "!**/*.d.ts",
        "!swa-cli.config.json"
      ],
      "cache": true
    },
    "test": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "!node_modules/**",
        "!.turbo/**",
        "!.vscode/**",
        "!tsconfig.json",
        "!tsconfig.app.json",
        "!tsconfig.node.json",
        "!**/*.md",
        "!**/docs/**",
        "!**/notes/**",
        "!**/*.log",
        "!.editorconfig",
        "!.prettierrc",
        "!.prettierignore",
        "!.cache/**",
        "!dist/**",
        "!build/**",
        "!.scannerwork/**",
        "!codeql-database/**",
        "!pnpm-lock.yaml",
        "!package-lock.json",
        "!swa-cli.config.json"
      ],
      "cache": true
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!vite.config.ts",
        "!vitest.config.ts",
        "!node_modules/**",
        "!.turbo/**",
        "!.vscode/**",
        "!**/*.md",
        "!**/docs/**",
        "!**/notes/**",
        "!**/*.log",
        "!.editorconfig",
        "!.prettierrc",
        "!.prettierignore",
        "!.cache/**",
        "!dist/**",
        "!build/**",
        "!.scannerwork/**",
        "!codeql-database/**",
        "!pnpm-lock.yaml",
        "!package-lock.json",
        "!swa-cli.config.json"
      ],
      "cache": true
    },
    "deploy": {
      "cache": false,
      "outputs": []
    }
  }
}
